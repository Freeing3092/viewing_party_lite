<% if session[:user_id].nil? %>
  <p><%= link_to 'Log In', '/login' %></p>
  <p><%= button_to 'Create New User', '/register', method: :get %></p>
<% else %>
  <p><%= link_to 'Log Out', '/logout' %></p>
  
  <div id="users">
      <ul>
          <% @users.each do |user| %>
              <li><%= user.email %></li>
          <% end %>
      </ul>
  </div>
<% end %>
